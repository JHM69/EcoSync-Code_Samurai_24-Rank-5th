<?xml version="1.0" encoding="utf-8"?><MotionScene xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:motion="http://schemas.android.com/apk/res-auto">    <Transition        motion:constraintSetEnd="@+id/end"        motion:constraintSetStart="@id/start"        motion:autoTransition="animateToEnd"        motion:duration="2000">        <KeyFrameSet />    </Transition>    <ConstraintSet android:id="@+id/start">        <Constraint            android:id="@+id/button2"            android:layout_height="0.001dp"            motion:layout_constraintEnd_toEndOf="parent"            motion:layout_constraintHorizontal_bias="0.498"            motion:layout_constraintStart_toStartOf="parent"            android:layout_width="wrap_content"            motion:layout_constraintTop_toTopOf="@+id/imageView3"            motion:layout_constraintBottom_toBottomOf="parent"            motion:layout_constraintVertical_bias="0.761" />        <Constraint            motion:layout_constraintEnd_toEndOf="parent"            android:layout_width="0dp"            android:layout_height="380dp"            motion:layout_constraintBottom_toBottomOf="parent"            motion:layout_constraintTop_toBottomOf="@+id/imageView10"            motion:layout_constraintHorizontal_bias="0.0"            motion:layout_constraintStart_toStartOf="parent"            android:id="@+id/imageView3"            motion:layout_constraintVertical_bias="1.0" />    </ConstraintSet>    <ConstraintSet android:id="@+id/end">        <Constraint            android:id="@+id/imageView10"            android:layout_height="wrap_content"            motion:layout_constraintStart_toStartOf="parent"            android:layout_width="0dp"            android:layout_marginStart="24dp"            android:layout_marginTop="32dp"            android:layout_marginEnd="24dp"            motion:layout_constraintEnd_toEndOf="parent"            motion:layout_constraintHorizontal_bias="0.0"            motion:layout_constraintTop_toTopOf="parent" />        <Constraint            android:id="@+id/button2"            motion:layout_constraintEnd_toEndOf="parent"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            motion:layout_constraintStart_toStartOf="parent"            android:layout_marginStart="16dp"            android:layout_marginEnd="16dp"            android:elevation="0dp"            motion:layout_constraintTop_toBottomOf="@+id/imageView10"            motion:layout_constraintBottom_toTopOf="@+id/imageView3"            android:layout_marginTop="8dp" />        <Constraint            android:id="@+id/imageView3"            android:layout_width="0dp"            android:layout_height="380dp"            motion:layout_constraintEnd_toEndOf="parent"            motion:layout_constraintHorizontal_bias="0.0"            motion:layout_constraintStart_toStartOf="parent"            motion:layout_constraintTop_toBottomOf="@+id/button2"            android:layout_marginTop="8dp" />    </ConstraintSet></MotionScene>