<?xml version="1.0" encoding="utf-8"?><androidx.constraintlayout.motion.widget.MotionLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="match_parent"    app:layoutDescription="@xml/activity_result_scene">    <TextView        android:id="@+id/topic"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:textSize="12sp"        android:textStyle="bold"        app:layout_constraintBottom_toTopOf="@+id/textView10"        app:layout_constraintEnd_toEndOf="@+id/textView10"        app:layout_constraintStart_toStartOf="@+id/textView10"        app:layout_constraintTop_toTopOf="@+id/linearLayout4" />    <TextView        android:id="@+id/myScore"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_marginTop="24dp"        android:fontFamily="@font/regular"        android:textColor="@color/colorAccentt"        android:textSize="40sp"        android:textStyle="bold"        app:layout_constraintEnd_toStartOf="@+id/textView9"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@+id/linearLayout4" />    <TextView        android:id="@+id/textView10"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:fontFamily="@font/regular"        android:text="VS"        android:textSize="18sp"        app:layout_constraintBottom_toBottomOf="@+id/linearLayout4"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        tools:ignore="HardcodedText"        tools:text="VS" />    <TextView        android:id="@+id/textView9"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_marginTop="24dp"        android:text=":"        android:textSize="40sp"        android:textStyle="bold"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@+id/linearLayout4"        tools:ignore="HardcodedText" />    <androidx.constraintlayout.widget.ConstraintLayout        android:id="@+id/linearLayout4"        android:layout_width="match_parent"        android:layout_height="0dp"        android:foreground="?selectableItemBackground"        android:visibility="visible"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@+id/toolbar2"        tools:ignore="MissingConstraints,UnusedAttribute"        tools:targetApi="m">    <de.hdodenhof.circleimageview.CircleImageView        android:id="@+id/thisUserImage"        android:layout_width="48dp"        android:layout_height="48dp"        android:layout_marginStart="8dp"        android:src="@drawable/ic_logo"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent"        app:srcCompat="@drawable/ic_logo" />    <de.hdodenhof.circleimageview.CircleImageView        android:id="@+id/otherUserImage"        android:layout_width="48dp"        android:layout_height="48dp"        android:layout_marginTop="8dp"        android:layout_marginEnd="8dp"        android:layout_marginBottom="8dp"        android:src="@drawable/ic_logo"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintTop_toTopOf="parent"        app:srcCompat="@drawable/ic_logo" />    <TextView        android:id="@+id/thisUserName"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_marginStart="4dp"        android:fontFamily="@font/regular"        android:textSize="19sp"        android:textStyle="bold"        app:layout_constraintStart_toEndOf="@+id/thisUserImage"        app:layout_constraintTop_toTopOf="@+id/thisUserImage" />    <TextView        android:id="@+id/thisUserLevel"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_marginStart="4dp"        android:fontFamily="@font/regular"        android:textSize="13sp"        app:layout_constraintBottom_toBottomOf="@+id/thisUserImage"        app:layout_constraintStart_toEndOf="@+id/thisUserImage"        app:layout_constraintTop_toBottomOf="@+id/thisUserName" />    <TextView        android:id="@+id/otherUserName"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_marginEnd="4dp"        android:fontFamily="@font/regular"        android:textSize="19sp"        android:textStyle="bold"        app:layout_constraintEnd_toStartOf="@+id/otherUserImage"        app:layout_constraintTop_toTopOf="@+id/otherUserImage" />    <TextView        android:id="@+id/otherUserLevel"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_marginEnd="4dp"        android:fontFamily="@font/regular"        android:textSize="13sp"        app:layout_constraintBottom_toBottomOf="@+id/otherUserImage"        app:layout_constraintEnd_toStartOf="@+id/otherUserImage"        app:layout_constraintTop_toBottomOf="@+id/otherUserName" />    </androidx.constraintlayout.widget.ConstraintLayout>    <TextView        android:id="@+id/otherScore"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_marginTop="24dp"        android:fontFamily="@font/regular"        android:textColor="@color/colorSec"        android:textSize="40sp"        android:textStyle="bold"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toEndOf="@+id/textView9"        app:layout_constraintTop_toBottomOf="@+id/linearLayout4" />    <TextView        android:id="@+id/resultText"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_marginTop="16dp"        android:textColor="@color/green"        android:textSize="22sp"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@+id/textView9" />    <TextView        android:id="@+id/textView12"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_marginTop="8dp"        android:textSize="14sp"        app:layout_constraintEnd_toEndOf="@+id/resultText"        app:layout_constraintStart_toStartOf="@+id/resultText"        app:layout_constraintTop_toBottomOf="@+id/resultText" />    <androidx.appcompat.widget.Toolbar        android:id="@+id/toolbar2"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:background="?colorPrimary"        android:minHeight="?attr/actionBarSize"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent"        app:popupTheme="@style/AppTheme"        app:theme="@style/ToolbarTheme"        app:title="Match Result"        app:titleTextColor="@color/white" />    <TextView        android:id="@+id/textView13"        android:layout_width="62dp"        android:layout_height="23dp"        android:layout_marginTop="8dp"        android:fontFamily="@font/regular"        android:text="My Result"        android:textColor="@color/colorAccentt"        android:textSize="12sp"        app:layout_constraintEnd_toStartOf="@+id/textView9"        app:layout_constraintHorizontal_bias="0.5"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@+id/textView12"        tools:ignore="HardcodedText" />    <TextView        android:id="@+id/sfaaew"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_marginTop="8dp"        android:fontFamily="@font/regular"        android:text="Opponent Result"        android:textColor="@color/colorSec"        android:textSize="12sp"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintHorizontal_bias="0.5"        app:layout_constraintStart_toEndOf="@+id/textView9"        app:layout_constraintTop_toBottomOf="@+id/textView12"        tools:ignore="HardcodedText" />    <com.google.android.material.button.MaterialButton        android:id="@+id/playAgain"        android:layout_width="0dp"        android:layout_height="wrap_content"        android:layout_marginStart="16dp"        android:layout_marginEnd="16dp"        android:layout_marginBottom="8dp"        android:backgroundTint="?colorPrimary"        android:text="Play Again"        android:textColor="@color/white"        app:iconTint="@color/white"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@+id/jhm69"        app:icon="@drawable/replay"        app:iconGravity="textStart"        app:layout_constraintVertical_bias="1.0"        tools:ignore="HardcodedText,RtlHardcoded" />    <androidx.recyclerview.widget.RecyclerView        android:id="@+id/jhm69"        android:layout_width="match_parent"        android:layout_height="0dp"        android:layout_marginStart="16dp"        android:layout_marginTop="8dp"        android:layout_marginEnd="16dp"        app:layout_constraintBottom_toTopOf="@+id/playAgain"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintHorizontal_bias="0.6"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@+id/textView13" /></androidx.constraintlayout.motion.widget.MotionLayout>